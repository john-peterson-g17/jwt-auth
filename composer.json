{
    "name": "john-peterson-g17/jwt-authentication",
    "description": "A framework agnostic php implementation of JWT authentication built on top of the firebase/php-jwt package",
    "type": "library",
    "autoload": {
        "psr-4": {
            "JohnPetersonG17\\JwtAuthentication\\": "src/",
            "JohnPetersonG17\\JwtAuthentication\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "John Peterson",
            "email": "peterson.glock17@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "require": {
        "firebase/php-jwt": "^6.10",
        "ramsey/uuid": "^4.7",
        "predis/predis": "^2.2"
    }
}
